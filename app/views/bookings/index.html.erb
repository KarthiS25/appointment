<h2>List of bookings</h2>
<% @bookings.each do |booking| %>
<p>
Start: <%= booking.start %>
</p>
<p>
End: <%= booking.end %>
</p>
<p>
Purpose: <%= booking.purpose %>
</p>
<p>
Room: <%#= link_to booking.room.name, booking.room%>
</p>
<% end %>
<%= button_to "Create New Booking", new_booking_path%>